{
    "manifest_version": 2,
    "name": "12ft.io",
    "version": "1.0",
  
    "description": "Add a button to the menu to open links on the 12ft.io proxy.",
  
    "icons": {
        "16": "icons/icon.svg",
        "32": "icons/icon.svg",
        "48": "icons/icon.svg",
        "96": "icons/icon.svg"
    },

    "permissions": [
        "<all_urls>",
        "contextMenus",
        "tabs"
    ],

    "background": {
        "scripts": ["12ft.js"]
    }
  }
  